<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="TractNote.App"
             RequestedThemeVariant="Default">
             <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->

    <Application.Styles>
        <FluentTheme />
    </Application.Styles>

	<Application.Resources>
		<FontFamily x:Key="RobotoRegular">/Assets/Fonts/Roboto-Regular.ttf#Roboto</FontFamily>
		<FontFamily x:Key="RobotoSemiBold">/Assets/Fonts/Roboto-SemiBold.ttf#Roboto</FontFamily>
	</Application.Resources>
</Application>
